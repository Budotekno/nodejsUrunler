
<br>
<br>

<div class="container">

    <a href="/adminYeniKullanici" class="btn btn-success">Yeni Kullanıcı</a>

    <br>
    <br>

    <table class="table table-striped">
        <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Kullanıcı Adı</th>
        <th scope="col">Şifre</th>
       
        <th scope="col">Sil</th>
        <th scope="col" >Güncelle</th>
        
      </tr>
    </thead>
    <tbody>
    
        <% let a =1 %> 
        <% data.forEach(i => { %>
     
            <tr>
                <th scope="row"><%= a %>  </th>
                <td><%= i.kullaniciAdi %> </td>
                <td>
                    <% for( let index = 0; index < i.sifre.length; index++ ) { %>
                        <span>*</span>
                    <% } %>
                <td>
                    <a class="btn btn-danger" href="/adminKullaniciSil/<%= i.id %> ">Sil</a>
                </td>
                <td>
                    <a class="btn btn-warning">Güncelle</a>
                </td>
               
              </tr>

              <% a++ %> 
        
        <% }) %>
     
      
      
    </tbody>
    
    </table>

</div>
